<h1>Food Entries</h1>

<% @entries.each do |fe| %>
  <div class="card">
    <div class="card-body">
      <div class="card-title"><%= fe.first %></div>
      <% fe.second.each do |ent| %>
        <% ent.foods.each do |food| %>
          <p><%= food.name %></p>
        <% end %>
      <% end %>
    </div>
  </div>
<% end %>